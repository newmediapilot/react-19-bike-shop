{
  "name": "react-19-bike-shop",
  "homepage": "https://newmediapilot.github.io/react-19-bike-shop",
  "private": true,
  "version": "0.0.45",
  "type": "module",
  "scripts": {
    "reset": "rm -rf node_modules && rm -rf ./*.lock && yarn install",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --fix",
    "preview": "npm run build && vite preview",
    "deploy": "bash scripts/deploy.sh",
    "docs": "node ./node_modules/jsdoc/jsdoc.js \"src/**/*.ts\" -r -d docs",
    "db": "node ./node_modules/json-server/lib/bin.js ./src/db.json",
    "gf": "bash scripts/git-flatten-branch.sh",
    "gw": "node scripts/git-watch.js",
    "pr": "prettier --write \"src/**/*\""
  },
  "dependencies": {
    "@eslint/js": "^9.25.0",
    "@heroicons/react": "^2.2.0",
    "@react-spring/web": "^10.0.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@skeletonlabs/skeleton": "^3.1.3",
    "@skeletonlabs/skeleton-react": "^1.2.2",
    "@tailwindcss/vite": "^4.1.8",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.4.1",
    "chokidar": "^4.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "jsdoc": "^4.0.4",
    "json-server": "^1.0.0-beta.3",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.1",
    "react-router-dom": "^7.6.1",
    "redux-dynamic-middlewares": "^2.2.0",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-cli": "^7.3.0"
  }
}
